import { OnChanges, ElementRef, NgZone } from '@angular/core';
import { JdenticonConfig } from 'jdenticon';
export declare class CanvasJdenticonDirective implements OnChanges {
    private el;
    private zone;
    private config;
    hash: string;
    value: any;
    width: string | number;
    height: string | number;
    constructor(el: ElementRef<HTMLCanvasElement>, zone: NgZone, config: JdenticonConfig);
    ngOnChanges(): void;
}
